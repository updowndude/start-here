    "url": "https://github.com/updowndude/start-here"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "xo": {
    "esnext": true,
    "globals": [
      "window",
      "THREE",
      "document",
      "React",
      "ReactDOM"
    ],
    "ignores": [
      "public/**"
    ],
    "extends": "xo-react",
    "overrides": [
      {
        "files": "js/stuff.js",
